{
  "compilerOptions": {
    "allowJs": true,
    "target": "es5",
    "module": "ES6",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "types": ["core-js", "node", "jasmine", "lodash", "core-decorators"],
    "lib": ["ES5", "ES6", "ES7", "dom"],
    "sourceMap": true,
    "downlevelIteration": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "skipLibCheck": true
  }
}
